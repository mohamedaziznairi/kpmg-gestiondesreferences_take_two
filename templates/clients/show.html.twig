{% extends 'base.html.twig' %}

{% block title %}Clients{% endblock %}

{% block body %}
    <h1>Clients</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idclient</th>
                <td>{{ client.idclient }}</td>
            </tr>
            <tr>
                <th>Companyname</th>
                <td>{{ client.companyname }}</td>
            </tr>
            <tr>
                <th>Addeddate</th>
                <td>{{ client.addeddate ? client.addeddate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_clients_index') }}">back to list</a>

    <a href="{{ path('app_clients_edit', {'idclient': client.idclient}) }}">edit</a>

    {{ include('clients/_delete_form.html.twig') }}
{% endblock %}
